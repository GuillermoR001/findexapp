{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "build-v-onboarding": "cd nova-components/VOnboarding && npm run dev",
        "build-v-onboarding-prod": "cd nova-components/VOnboarding && npm run prod",
        "build-fullcalendar": "cd nova-components/Fullcalendar && npm run dev",
        "build-fullcalendar-prod": "cd nova-components/Fullcalendar && npm run prod",
        "build-calendar": "cd nova-components/Calendar && npm run dev",
        "build-calendar-prod": "cd nova-components/Calendar && npm run prod"
    },
    "devDependencies": {
        "axios": "^0.25",
        "laravel-mix": "^6.0.6",
        "lodash": "^4.17.19",
        "postcss": "^8.1.14"
    },
    "dependencies": {
        "@fullcalendar/core": "^6.1.8",
        "@fullcalendar/daygrid": "^6.1.8",
        "@fullcalendar/interaction": "^6.1.8",
        "@fullcalendar/vue3": "^6.1.8",
        "v-onboarding": "^2.5.1"
    }
}
